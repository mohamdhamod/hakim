APP_NAME="Hakim Clinics"
DESCRIPTION="Hakim Clinics helps patients find trusted clinics and book appointments easily. Doctors can manage schedules, patients and examinations in one place."
APP_ENV=local
APP_KEY=base64:gEoIe2coRp0Z2Qn28WtubajvL5QvKU3VIZqfRgDdFNM=
APP_DEBUG=true
APP_URL=http://localhost

# Google OAuth (Laravel Socialite)
# Create credentials at https://console.cloud.google.com/apis/credentials
# Authorized redirect URI must match GOOGLE_REDIRECT_URI
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_REDIRECT_URI=${APP_URL}/auth/google/callback
LOGO_PATH="${APP_URL}/images/logo.svg"

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=hakim_clinics
DB_USERNAME=root
DB_PASSWORD=

BROADCAST_DRIVER=log

# Cache Configuration
# Options: file, redis, memcached, array
# For production, use redis for better performance
CACHE_DRIVER=file

FILESYSTEM_DISK=local

# Queue Configuration
# Options: sync (development), database, redis (production)
# Use 'database' or 'redis' for asynchronous job processing
# Don't forget to run: php artisan queue:work
QUEUE_CONNECTION=sync

SESSION_DRIVER=file
SESSION_LIFETIME=120

MEMCACHED_HOST=127.0.0.1

# Redis Configuration (Optional - for production caching/queues)
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# Mail Configuration
# For development, use Mailtrap: https://mailtrap.io
# For production, use SendGrid, Mailgun, SES, etc.
MAIL_MAILER=smtp
MAIL_HOST=smtp.mailtrap.io
MAIL_PORT=2525
MAIL_USERNAME=
MAIL_PASSWORD=
MAIL_ENCRYPTION=tls
MAIL_FROM_ADDRESS=noreply@hakim.com
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_HOST=
PUSHER_PORT=443
PUSHER_SCHEME=https
PUSHER_APP_CLUSTER=mt1

VITE_APP_NAME="${APP_NAME}"
VITE_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
VITE_PUSHER_HOST="${PUSHER_HOST}"
VITE_PUSHER_PORT="${PUSHER_PORT}"
VITE_PUSHER_SCHEME="${PUSHER_SCHEME}"
VITE_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"

RECAPTCHAV_SITEKEY=
RECAPTCHAV_SECRET=

